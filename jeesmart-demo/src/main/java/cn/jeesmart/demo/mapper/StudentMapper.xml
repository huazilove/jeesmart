<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.jeesmart.demo.entity.Student" >

    <select id="findById" resultType="cn.jeesmart.demo.entity.Student"  parameterType="java.lang.Integer">
       SELECT *
	   FROM student where no=#{no}
    </select>

</mapper>